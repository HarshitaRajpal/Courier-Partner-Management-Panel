generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Partner {
  id            String   @id @default(uuid())
  name          String
  email         String   @unique
  phone         String
  baseRate      Float
  deliverySpeed String
  rating        Float
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
